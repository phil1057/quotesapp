<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="add" named="True">
    <property name="height">720</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">QuotesApp</property>
    <property name="width">1080</property>
    <containerlayout manager="grid">
      <property name="propagate">False</property>
      <property type="row" id="2" name="uniform">1</property>
    </containerlayout>
    <child>
      <object class="tk.Frame" id="topFrame" named="True">
        <property name="height">85</property>
        <property name="width">1060</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">15</property>
          <property name="pady">5</property>
          <property name="row">0</property>
        </layout>
        <containerlayout manager="grid">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Radiobutton" id="translateEn" named="True">
            <property name="command" type="command" cbtype="simple">translate</property>
            <property name="text" translatable="yes">English</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Radiobutton" id="translateFr" named="True">
            <property name="command" type="command" cbtype="simple">translate</property>
            <property name="text" translatable="yes">Français</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="labelKeywords" named="True">
            <property name="text" translatable="yes">Mot-Clés</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entryKeywords" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="buttonKeywords" named="True">
            <property name="text" translatable="yes">Filtrer</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="labelTitre" named="True">
            <property name="font">{Arial} 36 {italic}</property>
            <property name="padx">400</property>
            <property name="text" translatable="yes">" Citations "</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="row">1</property>
              <property name="rowspan">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="middleFrame" named="True">
        <property name="height">30</property>
        <property name="width">1040</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
        <containerlayout manager="grid">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Button" id="ajouterButton" named="True">
            <property name="text" translatable="yes">add</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="modifierButton" named="True">
            <property name="text" translatable="yes">mod</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="deleteButton" named="True">
            <property name="text" translatable="yes">del</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="importButton" named="True">
            <property name="text" translatable="yes">imp</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="exportButton" named="True">
            <property name="text" translatable="yes">exp</property>
            <layout manager="grid">
              <property name="column">4</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="copyButton" named="True">
            <property name="text" translatable="yes">cop</property>
            <layout manager="grid">
              <property name="column">5</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="precedantButton" named="True">
            <property name="overrelief">flat</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Précédent</property>
            <property name="width">20</property>
            <layout manager="grid">
              <property name="column">6</property>
              <property name="padx">70</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="randomButton" named="True">
            <property name="text" translatable="yes">Aléatoire</property>
            <property name="width">20</property>
            <layout manager="grid">
              <property name="column">7</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="suivantButton" named="True">
            <property name="text" translatable="yes">Suivant</property>
            <property name="width">20</property>
            <layout manager="grid">
              <property name="column">8</property>
              <property name="padx">70</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="addFrame" named="True">
        <property name="height">570</property>
        <property name="width">1040</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">2</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Button" id="button2">
            <property name="cursor">arrow</property>
            <property name="text" translatable="yes">Importer un fichier CSV</property>
            <layout manager="pack">
              <property name="pady">200</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
